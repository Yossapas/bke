<%= form_for @calendar do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :start_available_date %><br />
    <%= f.datetime_select :start_available_date %>
  </p>
  <p>
    <%= f.label :end_available_date %><br />
    <%= f.datetime_select :end_available_date %>
  </p>
  <p>
    <%= f.label :room_id %><br />
    <%= f.select(:room_id, Room.all.collect {|room| [ room.id, room.id ] }, { :include_blank => true }) %>
  </p>
  
  <p><%= f.submit %></p>
<% end %>
